	_sw1	(a, 1),
	_pot0	(a, 2),
	_btn0	(a, 3),
	stream0(), 
	stream1()
{
	
	outputSocket.push_back(new OutputSocket(a, id___, dectid____, @@stream@@, ___, "name___"));
	inputSocket.push_back(new InputSocket(a, id___, dectid____, @@stream@@, ___, "name___"));
	
	internalConns.push_back(new AudioConnection(stream0,0, stream1,0));
}

